<form class="d-inline" method="post" action="{{ path('app_ticket_delete', {'annee': ticket.annee, 'numeroTicket': ticket.numeroTicket}) }}" onsubmit="return confirm('{% trans %}Are you sure you want to delete this item?{% endtrans %}'); ">
	<input type="hidden" name="_token" value="{{ csrf_token('delete' ~ ticket.annee) }}">
	<button class="btn btn-danger">{% trans %}Delete{% endtrans %}</button>
</form>

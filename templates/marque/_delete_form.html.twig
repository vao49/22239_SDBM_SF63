<form class="d-inline" method="post" action="{{ path('app_marque_delete', {'idMarque': marque.idMarque}) }}" onsubmit="return confirm('{% trans %}Are you sure you want to delete this item?{% endtrans %}');">
	<input type="hidden" name="_token" value="{{ csrf_token('delete' ~ marque.idMarque) }}">
	<button class="btn  btn-danger btn-sm {% trans %}icon delete{% endtrans %}">{% trans %}Delete{% endtrans %}</button>
</form>
